        <gmd:CI_Citation>
          <gmd:title>
            <gco:CharacterString>RV Investigator Voyage IN2017_V05 EM710 Multibeam Echosounder Data</gco:CharacterString>
          </gmd:title>
          <gmd:alternateTitle>
            <gco:CharacterString>IN2017_V05 EM710</gco:CharacterString>
          </gmd:alternateTitle>
          <gmd:date gco:nilReason="missing">
